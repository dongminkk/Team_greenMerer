<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="73dp"
        android:orientation="horizontal">

        <ImageButton
            android:id="@+id/back_to_timeline_btn"
            android:layout_width="130px"
            android:layout_height="140px"
            android:layout_marginLeft="60px"
            android:layout_gravity="center_vertical"
            android:background="#00ff0000"
            app:srcCompat="@drawable/arrow_back_ios_fill0_wght400_grad0_opsz48" />

        <TextView
            android:id="@+id/textView8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginLeft="240px"
            android:layout_marginTop="0px"
            android:text="타임라인"
            android:textSize="60px"
            android:textStyle="bold" />
    </LinearLayout>



    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="360px"
        android:orientation="vertical"
        android:background="@drawable/timeline_detail">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="130px"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/motorcycle_black"
                android:layout_width="100px"
                android:layout_height="120px"
                android:layout_marginLeft="40px"
                android:layout_marginTop="15px"
                app:srcCompat="@drawable/walk_black" />

            <TextView
                android:id="@+id/walk_text"
                android:layout_width="200px"
                android:layout_height="150px"
                android:layout_marginTop="30px"
                android:layout_marginLeft="5px"
                android:text="도보"
                android:textSize="70px"
                android:textStyle="bold" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="10px"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/walk_co2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="70px"
                android:layout_weight="1"
                android:text="0"
                android:textSize="80px"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/walk_gkm_text"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="g" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="80px"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/walk_km"
                android:layout_width="wrap_content"
                android:layout_height="80px"
                android:layout_marginLeft="70px"
                android:layout_weight="0"
                android:text="0"
                android:textSize="50px"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/walk_km_text"
                android:layout_width="wrap_content"
                android:layout_height="23dp"
                android:layout_weight="1"
                android:text="km" />

        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="360px"
        android:background="@drawable/timeline_detail"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="130px"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/car_black"
                android:layout_width="100px"
                android:layout_height="100px"
                android:layout_marginLeft="40px"
                android:layout_marginTop="25px"
                app:srcCompat="@drawable/car_black" />

            <TextView
                android:id="@+id/car"
                android:layout_width="300px"
                android:layout_height="100px"
                android:layout_marginLeft="10px"
                android:layout_marginTop="20px"
                android:text="승용차"
                android:textSize="70px"
                android:textStyle="bold" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="10px"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/car_co2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="70px"
                android:layout_weight="1"
                android:text="0"
                android:textSize="80px"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/car_gkm_text"
                android:layout_width="30px"
                android:layout_height="26dp"
                android:layout_weight="1"
                android:text="g" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="80px"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/car_km"
                android:layout_width="wrap_content"
                android:layout_height="80px"
                android:layout_marginLeft="70px"
                android:layout_weight="1"
                android:text="0"
                android:textSize="50px"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/car_km_text"
                android:layout_width="wrap_content"
                android:layout_height="23dp"
                android:layout_weight="1"
                android:text="km" />

        </LinearLayout>

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="360px"
        android:orientation="vertical"
        android:background="@drawable/timeline_detail">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="130px"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/moter_black"
                android:layout_width="100px"
                android:layout_height="100px"
                android:layout_marginLeft="40px"
                android:layout_marginTop="30px"
                app:srcCompat="@drawable/motorcycle_black" />

            <TextView
                android:id="@+id/moter"
                android:layout_width="350px"
                android:layout_height="100px"
                android:layout_marginTop="25px"
                android:layout_marginLeft="15px"
                android:text="자전거"
                android:textSize="70px"
                android:textStyle="bold" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="10px"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/moter_co2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="70px"
                android:layout_weight="1"
                android:text="0"
                android:textSize="80px"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/moter_gkm_text"
                android:layout_width="30px"
                android:layout_height="26dp"
                android:layout_weight="1"
                android:text="g" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="80px"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/moter_km"
                android:layout_width="wrap_content"
                android:layout_height="80px"
                android:layout_marginLeft="70px"
                android:layout_weight="1"
                android:text="0"
                android:textSize="50px"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/moter_km_text"
                android:layout_width="wrap_content"
                android:layout_height="23dp"
                android:layout_weight="1"
                android:text=" km" />

        </LinearLayout>
    </LinearLayout>

</LinearLayout>